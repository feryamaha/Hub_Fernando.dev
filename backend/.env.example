# Arquivo: backend/.env.example

# Configurações do Servidor
PORT=3001
NODE_ENV=development

# Configurações do PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=whff_db
POSTGRES_USER=seu_usuario
POSTGRES_PASSWORD=sua_senha

# Configurações da API do Google
GOOGLE_API_KEY=sua_chave_api_aqui
GOOGLE_SEARCH_ENGINE_ID=seu_search_engine_id_aqui

# Configurações da API do YouTube
YOUTUBE_API_KEY=sua_chave_api_aqui

# Configurações de Segurança
JWT_SECRET=seu_jwt_secret_aqui
RATE_LIMIT_WINDOW_MS=900000  # 15 minutos
RATE_LIMIT_MAX_REQUESTS=100  # Máximo de requisições por janela de tempo 